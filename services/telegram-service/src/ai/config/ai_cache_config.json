{
  "ai_cache": {
    "max_cache_files": 10,
    "cleanup_threshold": 10,
    "file_extensions": [
      ".json",
      ".txt",
      ".cache",
      ".tmp",
      ".data",
      ".result"
    ],
    "exclude_patterns": [
      "config",
      "backup",
      "important",
      "system",
      "permanent"
    ]
  },
  "prompt_cache": {
    "max_cache_files": 15,
    "cleanup_threshold": 15,
    "file_prefix": "AI_PROMPT_",
    "file_extensions": [
      ".txt",
      ".json"
    ],
    "exclude_patterns": [
      "backup",
      "important",
      "template"
    ]
  },
  "global": {
    "auto_cleanup_enabled": true,
    "cleanup_batch_size": 5,
    "cleanup_interval_hours": 1
  }
}